{
    "name": "mautic/core",
    "license": "GPL-3.0",
    "type": "mautic-core",
    "description": "Mautic Open Source Distribution",
    "autoload": {
        "psr-4": {
            "Mautic\\": "app/bundles/",
            "MauticPlugin\\": "plugins/",
            "Mautic\\Middleware\\": "app/middlewares/"
        },
        "files": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "php": ">=7.2.21 <7.4",
        "ext-zlib": "^7.2",
        "ext-zip": "^1.15",
        "ext-imap": "*",
        "symfony/console": "~3.4.0",
