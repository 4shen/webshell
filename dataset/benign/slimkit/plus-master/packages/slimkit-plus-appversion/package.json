{
  "name": "plus-appversion",
  "version": "0.0.1",
  "description": "a component of thinksns+ for manage app version",
  "main": "webpack.config.babel.js",
  "scripts": {
    "clean-dist": "trash dist/*",
    "dev": "npm run clean-dist && cross-env NODE_ENV=development webpack --progress --hide-modules --colors",
    "watch": "npm run dev -- -w",
    "hot": "npm run clean-dist && cross-env NODE_ENV=development webpack-dev-server --hot --colors",
    "dist": "npm run clean-dist && cross-env NODE_ENV=production webpack --display-error-details --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slimkit/plus-appversion.git"
  },
  "keywords": [
    "thinksns+",
    "component"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/slimkit/plus-appversion/issues"
  },
  "homepage": "https://github.com/slimkit/plus-appversion#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "element-ui": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "plus-message-bundle": "^0.0.5",
    "trash-cli": "^1.4.0",
    "vue": "^2.1.6",
    "vue-router": "^3.0.1",
    "vue-timeago": "^3.3.6",
    "webpack-laravel-mix-manifest": "^1.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-vue-app": "^1.2.0",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "style-loader": "^0.13.1",
    "vue-loader": "^9.8.0",
    "webpack": "^3.0.0-rc.2"
  }
}
