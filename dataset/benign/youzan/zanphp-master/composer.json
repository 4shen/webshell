{
    "name": "zanphp/zan",
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    },
    "require": {
        "zanphp/tcp-server": "dev-master",
        "zanphp/http-server": "dev-master",
        "zanphp/mq-server": "dev-master",
        "zanphp/ws-server": "dev-master"
    },
    "autoload": {
        "psr-4": {
        },
        "classmap": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zan\\Framework\\Test\\": "test/"
        },
        "classmap": [
            "test/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
