{contentType xml}
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
<url>
    <loc>{$baseUrl}/index.html</loc>
</url>
<url n:foreach="$allNamespaces as $namespace">
    <loc>{$baseUrl}/{$namespace|linkNamespace}</loc>
</url>

{define elements}
<url n:foreach="$elements as $element">
    <loc>{$baseUrl}/{$element|linkReflection}</loc>
</url>
{/define}

{include elements, elements => $allClasses}
{include elements, elements => $allInterfaces}
{include elements, elements => $allTraits}
{include elements, elements => $allFunctions}
</urlset>
