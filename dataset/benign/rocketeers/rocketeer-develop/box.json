{
    "chmod": "0755",
    "banner-file": "bin/banner.txt",
    "extract": true,
    "bootstrap": "src/Rocketeer/Console/Compilation/WhitespaceCompactor.php",
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Rocketeer\\Console\\Compilation\\WhitespaceCompactor"
    ],
    "directories": [
        "src"
    ],
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": "/\\.(php|json|exe)/",
            "exclude": [
                "escapestudios",
                "friendsofphp",
                "johnkary",
                "myclabs",
                "php-vfs",
                "phpdocumentor",
                "phpspec",
                "phpunit",
                "sebastian",
                "squizlabs",
                "tedivm",
                "test",
                "tests",
                "Tests",
                "webmozart"
            ],
            "in": "vendor"
        }
    ],
    "git-version": "commit",
    "main": "bin/rocketeer",
    "output": "bin/rocketeer.phar",
    "stub": true
}
