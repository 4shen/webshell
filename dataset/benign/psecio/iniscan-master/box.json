{
    "algorithm": "SHA1",
    "alias": "iniscan.phar",
    "chmod": "0755",
    "directories": ["src", "bin"],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests", "Test", "test", ".git"],
            "in": "vendor"
        }
    ],
    "main": "bin/iniscan",
    "output": "iniscan.phar",
    "stub": true
}
