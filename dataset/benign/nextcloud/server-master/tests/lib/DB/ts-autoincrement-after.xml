<?xml version="1.0" encoding="utf-8" ?>
<database>

	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>

	<charset>utf8</charset>

	<table>

		<name>*dbprefix*table</name>

		<declaration>
			<field>
				<name>auto_id</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<autoincrement>1</autoincrement>
				<length>4</length>
			</field>
			<field>
				<name>textfield</name>
				<type>text</type>
				<default>foo</default>
				<notnull>true</notnull>
				<length>32</length>
			</field>
		</declaration>
	</table>
</database>
