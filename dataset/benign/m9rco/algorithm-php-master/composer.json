{
  "name": "algorithm-php",
  "description": "algorithm-php",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "pushaowei"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "~0.9"
  },
  "autoload": {
    "classmap": [
      "./src"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  },
  "require": {
    "ext-json": "*",
    "ext-ctype": "*",
    "ext-openssl": "*",
    "php": ">=5.6.4",
    "ext-curl": "*",
    "stojg/recommend": "^1.0",
    "ext-iconv": "*"
  }
}
