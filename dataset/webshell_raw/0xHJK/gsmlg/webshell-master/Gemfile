# A sample Gemfile
source "https://rubygems.org"


group :app do
    gem 'rack'
    gem 'sinatra', :require => 'sinatra/base'
    gem 'sinatra-contrib'
    gem 'thin'
    gem 'sprockets'
    gem 'sprockets-helpers'
    gem 'erubis'
    gem 'browser'
    gem 'coffee-script'
    gem 'sass'
end

group :production do
  gem 'uglifier'
end

group :development do
  gem 'better_errors'
end
